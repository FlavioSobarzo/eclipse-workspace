
public class respuesta {

	public static void main(String[] args) {
		int i = 1;
		double d = 0.1;
		String s = "Hola";
		char c = 'c';
		boolean b = true;
		
		System.out.println("int con double " + (i+d));
		System.out.println("int con String " + (i+s));
		System.out.println("int con char " + (i+c));
		// System.out.println("int con boolean " + (i+b)); //No se permite
		System.out.println("double con String " + (d+s));
		System.out.println("double con char " + (d+c));
		// System.out.println("double con boolean " + (d+b)); //No se permite 
		System.out.println("String con char " + (s+c));
		System.out.println("String con boolean " + (s+b));
		// System.out.println("char con boolean " + (c+b)); //No se permite
		

	}

}
