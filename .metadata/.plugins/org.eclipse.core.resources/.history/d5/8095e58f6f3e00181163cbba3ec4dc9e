package Asistente;

import java.util.Date;

public class Home {
	public static void main(String[] args) {

		Date tiempo = new Date();
	    int segundo = tiempo.getSeconds();
	    int minuto = tiempo.getMinutes();
	    double horaActual = minuto + segundo/100;
	    System.out.print("hora actual = ");
	    System.out.println(horaActual);
	    Noticias hora = new Noticias(horaActual);

    }
}